<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TechAndTools</title>

    <link href="~/css/font-awesome.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Hind:400,700&display=swap" rel="stylesheet">
    <link type="text/css" rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link type="text/css" rel="stylesheet" href="~/css/style.css" />
</head>
<body>

    <partial name="Partials/_NavbarPartial" />
    <partial name="Partials/_CookieConsentPartial" />

    @await Component.InvokeAsync("CategoriesNavigation")

    <div class="container">
        <div class="pb-3">
            @RenderBody()
        </div>
    </div>
    <partial name="Partials/_FooterPartial" />

    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.zoom.min.js"></script>
    <script src="~/js/main.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>